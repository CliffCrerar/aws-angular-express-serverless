{
	"name": "aws-angular-serverless-express",
	"description": "Hello World ExpressJS WebService",
	"private": true,
	"main": "app.js",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --aot=true --proxyConfig=src/proxy.conf.js --hmr=true --prod=false --verbose=false",
		"start:dev": "ng build --prod=false --watch=true",
		"build": "ng build",
		"test": "ng test --watch=false --browsers=ChromeHeadless",
		"test:dev": "ng test",
		"dev": "concurrently \"npm start\" \"sam local start-api\"",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"api": "sam local start-api",
		"startdb": "aws rds start-db-instance --db-instance-identifier properties",
		"stopdb": "aws rds stop-db-instance --db-instance-identifier properties",
		"dbstate": "aws rds describe-db-instances --db-instance-identifier properties | grep DBInstanceStatus"
	},
	"dependencies": {
		"@angular/animations": "~9.0.3",
		"@angular/cdk": "^9.1.1",
		"@angular/common": "~9.0.3",
		"@angular/compiler": "~9.0.3",
		"@angular/core": "~9.0.3",
		"@angular/forms": "~9.0.3",
		"@angular/material": "^9.1.1",
		"@angular/platform-browser": "~9.0.3",
		"@angular/platform-browser-dynamic": "~9.0.3",
		"@angular/router": "~9.0.3",
		"aws-serverless-express": "3.1.3",
		"concurrently": "^5.1.0",
		"dotenv": "^8.2.0",
		"express": "4.17.1",
		"pg": "^7.18.2",
		"rxjs": "~6.5.4",
		"tslib": "^1.10.0",
		"web-animations-js": "^2.3.2",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.900.4",
		"@angular/cli": "~9.0.4",
		"@angular/compiler-cli": "~9.0.3",
		"@angular/language-service": "~9.0.3",
		"@types/jasmine": "~3.5.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^12.11.1",
		"codelyzer": "^5.1.2",
		"intl": "^1.2.5",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.3.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~2.1.0",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"protractor": "~5.4.3",
		"ts-node": "~8.3.0",
		"tslint": "~5.18.0",
		"typescript": "~3.7.5"
	}
}
